<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="iit_dhanbad.teamrocket.alpha_cogn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="iit_dhanbad.teamrocket.alpha_cogn.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="iit_dhanbad.teamrocket.alpha_cogn.MainActivity"
            android:label="@string/title_activity_main"
            android:parentActivityName="iit_dhanbad.teamrocket.alpha_cogn.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="iit_dhanbad.teamrocket.alpha_cogn.LoginActivity" />
        </activity>
    </application>

</manifest>